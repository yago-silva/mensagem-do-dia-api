<?xml version="1.0" encoding="utf-8"?>
<databaseChangeLog
    xmlns="http://www.liquibase.org/xml/ns/dbchangelog"
    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xsi:schemaLocation="http://www.liquibase.org/xml/ns/dbchangelog http://www.liquibase.org/xml/ns/dbchangelog/dbchangelog-latest.xsd">
    <!--
        Added the constraints for entity Media.
    -->
    <changeSet id="20240203032204-2" author="jhipster">

        <addForeignKeyConstraint baseColumnNames="owner_id"
                                 baseTableName="media"
                                 constraintName="fk_media__owner_id"
                                 referencedColumnNames="id"
                                 referencedTableName="jhi_user"
                                 />

        <addForeignKeyConstraint baseColumnNames="phrase_id"
                                 baseTableName="media"
                                 constraintName="fk_media__phrase_id"
                                 referencedColumnNames="id"
                                 referencedTableName="phrase"
                                 />

        <addForeignKeyConstraint baseColumnNames="ad_id"
                                 baseTableName="media"
                                 constraintName="fk_media__ad_id"
                                 referencedColumnNames="id"
                                 referencedTableName="ad"
                                 />

        <addForeignKeyConstraint baseColumnNames="category_id"
                                 baseTableName="media"
                                 constraintName="fk_media__category_id"
                                 referencedColumnNames="id"
                                 referencedTableName="category"
                                 />

        <addForeignKeyConstraint baseColumnNames="tag_id"
                                 baseTableName="media"
                                 constraintName="fk_media__tag_id"
                                 referencedColumnNames="id"
                                 referencedTableName="tag"
                                 />
    </changeSet>
</databaseChangeLog>
